{
  "name": "@spacetimexyz/parser",
  "version": "0.1.27",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/* &",
    "build": "yarn build:nearley && yarn build:tsc",
    "build:tsc": "tsc",
    "build:nearley": "nearleyc ./src/lexer.ne -o ./src/lexer.ts && yarn fix",
    "test": "yarn build && jest",
    "prepublish": "yarn build",
    "check-types": "tsc",
    "fix": "eslint \"./src/**/*.{ts,tsx}\" --fix"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "lodash": "^4.17.21",
    "moo": "^0.5.1",
    "nearley": "^2.20.1"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "<rootDir>/**/*.spec.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/nearley": "^2.11.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.2",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "typescript": "^4.2.3",
    "watch": "^1.0.2"
  }
}
