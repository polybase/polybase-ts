{
  "name": "@spacetimexyz/parser",
  "version": "0.1.27",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "watch 'yarn build' ./src",
    "build": "yarn build:babel && yarn build:nearley",
    "build:babel": "babel src --ignore .spec.js,__mocks__ -d dist --extensions '.ts,.js' --copy-files && yarn build:types",
    "build:nearley": "nearleyc ./src/lexer.ne -o ./dist/lexer.js",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "yarn build && jest",
    "test:watch": "npm-run-all -p start test ",
    "debug": "yarn build && nearley-test ./dist/lexer.js --input",
    "prepublish": "yarn build",
    "check-types": "tsc"
  },
  "private": true,
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "lodash": "^4.17.21",
    "moo": "^0.5.1",
    "nearley": "^2.20.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/nearley": "^2.11.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-source-map-support": "^2.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.21.3",
    "jest": "^26.5.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.2.3",
    "watch": "^1.0.2"
  }
}
