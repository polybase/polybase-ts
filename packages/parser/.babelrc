{
  "presets": [
    "@babel/preset-env", 
    "@babel/typescript"
  ],
  "plugins": [
    "source-map-support",
    "@babel/transform-runtime",
    "transform-export-extensions",
    [
      "module-resolver",
      {
        "alias": {
          "test": "./test",
          "#": "./src"
        },
        "extensions": [
          ".js",
          ".jsx",
          ".es",
          ".es6",
          ".mjs",
          ".ts",
          ".tsx"
        ],
        "stripExtensions": [
          ".js",
          ".jsx",
          ".es",
          ".es6",
          ".mjs",
          ".ts",
          ".tsx"
        ]
      }
    ]
  ],
  "ignore": [
    "**/*.d.ts"
  ]
}